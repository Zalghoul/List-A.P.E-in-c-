#include <stdio.h>
int main() {
    int num, maior, menor;
    scanf("%d", &num);
    maior = num;
    menor = num;

    while (num >= 0) {
        if (num > maior) {
            maior = num;
        }
        if (num < menor) {
            menor = num;
        }
        scanf("%d", &num);
    }

    printf("Maior: %d\nMenor: %d\n", maior, menor);

    return 0;
}
