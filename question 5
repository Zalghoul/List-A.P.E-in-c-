#include <stdio.h>

int main() {
    int num;
    printf("Digite um numero positivo: ");
    scanf("%d", &number);

    printf("Os divisores do numero %d sao: ", number);
    for (int i = 1; i <= number; i++) {
        if (number % i == 0) {
            printf("%d ", i);
        }
    }

    printf("\n");

    return 0;
}
